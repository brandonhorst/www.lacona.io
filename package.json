{
  "name": "www.lacona.io",
  "version": "0.1.2",
  "description": "Lacona App",
  "main": "index.js",
  "scripts": {
    "build:html": "babel-node render.jsx",
    "build:style": "sass front/style.scss public/style.css -t compressed --sourcemap=none",
    "build:script": "browserify front/script.jsx -t babelify -g browserify-shim -r lacona-api -r lacona-phrases -r elliptical --extension=jsx | uglifyjs -o public/script.min.js",
    "build:vendor": "cp node_modules/moment/min/moment.min.js public & cp node_modules/react/dist/react.min.js public",
    "build": "npm run build:html & npm run build:vendor & npm run build:style & npm run build:script & wait",
    "open": "open public/index.html"
  },
  "dependencies": {
    "elliptical": "^1.0.0-rc13",
    "elliptical-extend": "^1.0.0-rc7",
    "elliptical-observe": "^1.0.0-rc7",
    "elliptical-wormhole": "^1.0.0-rc3",
    "lacona-api": "^0.4.1",
    "lacona-command": "^0.3.0",
    "lacona-communicate": "^0.5.0",
    "lacona-events": "^0.3.0",
    "lacona-finder": "^0.3.0",
    "lacona-itunes": "0.1.0",
    "lacona-osx": "^0.2.0",
    "lacona-phrases": "0.6.0",
    "lacona-search-internet": "^0.2.0",
    "lacona-settings": "^0.2.0",
    "lacona-translate": "^0.3.0",
    "lacona-utils": "^0.2.0",
    "lodash": "^4.13.1",
    "moment": "^2.13.0",
    "moment-duration-format": "^1.3.0",
    "react": "^15.1.0",
    "react-lacona": "^0.6.0",
    "rxjs": "^5.0.0-beta.8"
  },
  "browserify-shim": {
    "react": "global:React",
    "lodash": "global:_",
    "moment": "global:moment",
    "react-dom": "global:ReactDOM"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/laconalabs/www.lacona.io.git"
  },
  "keywords": [
    "lacona"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "browserify-shim": "^3.8.12",
    "rimraf": "^2.5.2",
    "uglify-js": "^2.6.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx"
    ]
  }
}
